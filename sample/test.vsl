VAR i
VAR j
VAR k
VAR e
FUNC  main()
{
    VAR i
    i := 0
    j := 1

    WHILE 11 - i
    DO
    {
        PRINT "f( ", i, " ) = ", f( i ), "\n"
        i := i + 1
    }
    DONE
    PRINT "success!\n"
}

FUNC  f( n )
    IF n
    THEN
        RETURN n * f( n - 1 )
    ELSE
        RETURN 1
    FI

